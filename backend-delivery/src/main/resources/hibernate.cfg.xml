<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
		"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.connection.driver_class">com.microsoft.sqlserver.jdbc.SQLServerDriver</property>
        <property name="hibernate.connection.password">ramk</property>
        <property name="hibernate.connection.url">jdbc:sqlserver://localhost:1433;DatabaseName=firstdb;SelectMethod=cursor;user=sa;password=ramk;</property>
        <property name="hibernate.connection.username">sa</property>
        <property name="hibernate.dialect">org.hibernate.dialect.SQLServerDialect</property>
        <property name="hibernate.current_session_context_class">thread</property>
        <mapping class="com.delivery.backend.daos.Users"/>
        <mapping class="com.delivery.backend.daos.DeliveryPrint"/>
        <mapping class="com.delivery.backend.daos.DeliveryStatus"/>
        <mapping class="com.delivery.backend.daos.Employee"/>
        <mapping class="com.delivery.backend.daos.LiveStatus"/>
        <mapping class="com.delivery.backend.daos.MST_ID"/>
        <mapping class="com.delivery.backend.daos.MstStatus"/>
        <mapping class="com.delivery.backend.daos.Relation"/>
        <mapping class="com.delivery.backend.daos.UnDeliveryStatus"/>
    </session-factory>
</hibernate-configuration>
